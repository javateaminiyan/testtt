<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.examp.three.activity.Property.NameTransfer"
    android:id="@+id/rl_root">
    <RelativeLayout
        android:id="@+id/relativetoolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/li_toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimen_170dp"
            android:background="@color/colorPrimary"
            android:orientation="vertical">

                <android.support.v7.widget.Toolbar
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/toolbar"
                    android:fontFamily="@font/roboto_medium"
                    android:theme="@style/AppTheme.AppBarOverlay"
                    android:layout_marginTop="@dimen/dimen_20dp"/>
        </LinearLayout>
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none"
        android:layout_below="@+id/relativetoolbar"
        android:layout_marginTop="-80dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dimen_8dp"
            app:cardCornerRadius="@dimen/dimen_8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dimen_8dp"
                android:layout_marginLeft="@dimen/dimen_16dp"
                android:layout_marginRight="@dimen/dimen_16dp"
                android:orientation="vertical">

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/et_district"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawableRight="@drawable/ic_arrow_drop_down_black_24dp"
                    style="@style/sTIEditText"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    android:fontFamily="@font/roboto_medium"
                    android:hint="@string/district" />

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/et_panchayat"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:drawableRight="@drawable/ic_arrow_drop_down_black_24dp"
                    style="@style/sTIEditText"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    android:fontFamily="@font/roboto_medium"
                    android:hint="@string/panchayat" />

                <android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp">
                    <EditText
                        android:layout_width="match_parent"
                        android:inputType="number"
                        android:hint="@string/assno"
                        android:textSize="@dimen/dimen_14sp"
                        android:id="@+id/ed_taxno"
                        android:fontFamily="@font/roboto_medium"
                        android:layout_height="wrap_content" />

                </android.support.design.widget.TextInputLayout>

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginBottom="@dimen/dimen_8dp"
                    android:layout_marginTop="@dimen/dimen_8dp"
                    android:background="@color/button_color"
                    android:fontFamily="@font/roboto_medium"                    android:text="@string/submit"
                    android:id="@+id/button_submit_name_transfer"
                    android:textColor="@color/white" />

            </LinearLayout>


        </android.support.v7.widget.CardView>
    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/ll_assesmentdetails"
        android:visibility="gone"
        android:orientation="vertical">

        <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dimen_8dp"
            app:cardCornerRadius="@dimen/dimen_8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dimen_8dp"
                android:layout_marginTop="@dimen/dimen_8dp"
                android:orientation="vertical">


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/dimen_10dp"
                    android:gravity="center"
                    android:fontFamily="@font/roboto_medium"
                    style="@style/sTextView"
                    android:text="Assessment Details"/>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dimen_1dp"
                    android:background="@color/grey" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_margin="@dimen/dimen_5dp"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_30dp"
                        android:layout_weight="1"
                        style="@style/sTextView14"
                        android:fontFamily="@font/roboto_medium"
                        android:text="@string/name" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_30dp"
                        android:layout_weight="1"
                        style="@style/sTextView14"
                        android:text="@string/name"
                        android:fontFamily="@font/roboto_medium"
                        android:id="@+id/tv_name"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/dimen_5dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_30dp"
                        android:layout_weight="1"
                        style="@style/sTextView14"
                        android:fontFamily="@font/roboto_medium"
                        android:text="@string/wardno"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_30dp"
                        android:layout_weight="1"
                        android:id="@+id/tv_wardno"
                        android:text="@string/wardno"
                        android:fontFamily="@font/roboto_medium"
                        style="@style/sTextView14"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/dimen_5dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_30dp"
                        android:layout_weight="1"
                        android:text="@string/door_no"
                        android:fontFamily="@font/roboto_medium"
                        style="@style/sTextView14"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_30dp"
                        android:layout_weight="1"
                        android:id="@+id/tv_doorno"
                        android:fontFamily="@font/roboto_medium"
                        style="@style/sTextView14"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/dimen_5dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dimen_30dp"
                        android:layout_weight="1"
                        android:text="@string/streetname"
                        android:fontFamily="@font/roboto_medium"
                        style="@style/sTextView14"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dimen_30dp"
                        android:layout_weight="1"
                        android:textSize="@dimen/dimen_14sp"
                        android:id="@+id/tv_streetname"
                        android:fontFamily="@font/avvaiyar"
                        android:text="@string/streetname"
                        android:textColor="@color/black" />

                </LinearLayout>

            </LinearLayout>
        </android.support.v7.widget.CardView>
    </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/ll_nametransfer"
            android:visibility="gone"
            android:orientation="vertical">

            <android.support.v7.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/dimen_8dp"
                app:contentPadding="@dimen/dimen_20dp"
                app:cardCornerRadius="@dimen/dimen_8dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <android.support.design.widget.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <EditText
                            android:layout_width="match_parent"
                            android:inputType="number"
                            android:textSize="@dimen/dimen_14sp"
                            android:hint="@string/mobileno"
                            android:maxLength="10"
                            android:fontFamily="@font/roboto_medium"
                            android:id="@+id/et_mobile_no"
                            android:layout_height="wrap_content" />

                    </android.support.design.widget.TextInputLayout>

                    <android.support.design.widget.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimen_8dp">

                        <EditText
                            android:layout_width="match_parent"
                            android:textSize="@dimen/dimen_14sp"
                            android:fontFamily="@font/roboto_medium"                            android:hint="@string/emailId"
                            android:id="@+id/et_email_id"
                            android:inputType="textEmailAddress"
                            android:layout_height="wrap_content" />

                    </android.support.design.widget.TextInputLayout>

                    <android.support.design.widget.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dimen_8dp">

                        <EditText
                            android:layout_width="match_parent"
                            android:textSize="@dimen/dimen_14sp"
                            android:fontFamily="@font/roboto_medium"                            android:hint="@string/trname"
                            android:id="@+id/et_transfer_name"
                            android:layout_height="wrap_content" />

                    </android.support.design.widget.TextInputLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@style/sTextView"
                        android:drawableRight="@drawable/ic_asteriskneww"
                        android:drawablePadding="@dimen/dimen_5dp"
                        android:layout_marginTop="@dimen/dimen_8dp"
                        android:fontFamily="@font/roboto_medium"
                        android:text="@string/reg_doc"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/dimen_5dp"
                        android:background="@drawable/edittext_background"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/dimen_5dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/roboto_medium"                            android:gravity="start"
                            android:padding="@dimen/dimen_5dp"
                            android:id="@+id/ed_choosen_file_one"
                            android:text="@string/choosefile"
                            android:textAlignment="viewStart"
                            android:textSize="@dimen/fragment_body_text_size"
                            android:textStyle="italic|bold" />

                        <TextView
                            android:id="@+id/tv_upload_one"
                            android:textColor="@color/colorPrimaryDark"
                            android:text="@string/upload"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/dimen_5dp"
                            android:layout_weight="2"
                            android:enabled="true"
                            android:fontFamily="@font/roboto_medium"                            android:padding="@dimen/dimen_5dp"
                            android:textAlignment="center"
                            android:textSize="@dimen/fragment_body_text_size"
                            android:textStyle="italic|bold" />

                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/sTextView"
                        android:layout_marginTop="@dimen/dimen_8dp"
                        android:text="@string/name_legel_doc"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/dimen_5dp"
                        android:background="@drawable/edittext_background"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/dimen_5dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/roboto_medium"                            android:gravity="start"
                            android:padding="@dimen/dimen_5dp"
                            android:id="@+id/ed_choosen_file_two"
                            android:text="@string/choosefile"
                            android:textAlignment="viewStart"
                            android:textSize="@dimen/fragment_body_text_size"
                            android:textStyle="italic|bold" />

                        <TextView
                            android:id="@+id/tv_upload_two"
                            android:textColor="@color/colorPrimaryDark"
                            android:text="@string/upload"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/dimen_5dp"
                            android:layout_weight="2"
                            android:enabled="true"
                            android:fontFamily="@font/roboto_medium"                            android:padding="@dimen/dimen_5dp"
                            android:textAlignment="center"
                            android:textSize="@dimen/fragment_body_text_size"
                            android:textStyle="italic|bold" />


                    </LinearLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        style="@style/sTextView"
                        android:layout_marginTop="@dimen/dimen_8dp"
                        android:text="@string/death_certificate"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/dimen_5dp"
                        android:background="@drawable/edittext_background"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/dimen_5dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/roboto_medium"                            android:gravity="start"
                            android:padding="@dimen/dimen_5dp"
                            android:id="@+id/tv_choosen_file"
                            android:text="@string/choosefile"
                            android:textSize="@dimen/fragment_body_text_size"
                            android:textStyle="italic|bold"/>

                        <TextView
                            android:id="@+id/tv_upload_three"
                            android:textColor="@color/colorPrimaryDark"
                            android:text="@string/upload"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/dimen_5dp"
                            android:layout_weight="2"
                            android:enabled="true"
                            android:fontFamily="@font/roboto_medium"                            android:padding="@dimen/dimen_5dp"
                            android:textAlignment="center"
                            android:textSize="@dimen/fragment_body_text_size"
                            android:textStyle="italic|bold" />

                    </LinearLayout>

                    <Button
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dimen_30dp"
                        android:layout_marginBottom="@dimen/dimen_8dp"
                        android:layout_marginTop="@dimen/dimen_8dp"
                        android:textSize="@dimen/dimen_14sp"
                        android:background="@color/button_color"
                        android:fontFamily="@font/roboto_medium"                        android:text="@string/submit"
                        android:id="@+id/submit_nametransfer"
                        android:textColor="@color/white" />

                </LinearLayout>

            </android.support.v7.widget.CardView>

        </LinearLayout>

    </LinearLayout>
    </RelativeLayout>
    </ScrollView>

</RelativeLayout>